<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>Rejuvena.Collate.ExampleMod</AssemblyName>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    
    <!-- Mod properties/ -->
    <PropertyGroup>
        <Version>1.0.0</Version>
        <DisplayName>Rejuvena.Collate Example Mod</DisplayName>
        <Author>Tomat</Author>
        <Homepage>https://github.com/rejuvena/collate</Homepage>
        <HideCode>false</HideCode>
        <HideResources>false</HideResources>
        <IncludeSource>true</IncludeSource>
        <IncludePDB>true</IncludePDB>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Rejuvena.Collate" Version="1.0.0" />
    </ItemGroup>

</Project>
